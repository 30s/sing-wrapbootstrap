{
  "name": "sing-wrapbootstrap",
  "version": "0.0.0",
  "homepage": "https://github.com/neodevelop/sing-wrapbootstrap",
  "authors": [
    "neodevelop <juan@makingdevs.com>"
  ],
  "description": "Sing theme with Bootstrap",
  "license": "MIT",
  "private": true,
  "main": [
    "assets/javascripts/*.js",
    "assets/sass/*.scss",
    "assets/stylesheets/fonts/google/*.*"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "**/*.rb",
    "Gemfile*",
    "config.ru",
    "views",
    "img",
    "stylesheets/*.css"

  ],
  "dependencies": {
    "jquery": "~2.1.4",
    "bootstrap-sass": "~3.3.6",
    "font-awesome": "~4.6.3",
    "animate.css": "~3.5.2",
    "awesome-bootstrap-checkbox": "~0.3.7",
    "pace.js": "git://github.com/HubSpot/pace.git#v0.7.1",
    "slimScroll": "~1.3.8",
    "widgster": "~0.0.3",
    "jquery-touchswipe": "~1.6.18",
    "jquery-pjax": "~1.9.6"
  }
}
